<html>
    <head>
        <title>companys</title>
    </head>
    <body>
        <h1>companys</h1>
        <table border="1">
            <tr><th>company ID</th><th>Name</th><th>address_id</th></tr>
            <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><%= result[i].company_id%></td>
                <td><a href="/company/?company_id=<%= result[i].company_id %>"><%= result[i].company_name %></a></td>
                <td><%= result[i].address_id %></td>
            </tr>
            <% } %>
        </table>
    </body>

</html>